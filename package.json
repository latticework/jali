{
  "//": [
    "syntax-trailing-function-commas",
    "transform-async-to-generator",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-es2015-function-name",
    "transform-function-sent",
    "transform-exponentiation-operator"
  ],
  "name": "jali-srcs",
  "version": "0.0.0",
  "description": "Cross-platform service specification and execution context. Jali is the JavaScript implementation.",
  "scripts": {
    "start": "ng server",
    "postinstall": "typings install",
    "lint": "tslint \"packages/**/*.ts\"",
    "build": "tsc -p packages",
    "test": "BABEL_ENV=test ava dist/all/**/*.test.js --concurrency 5",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/latticework/jali.git"
  },
  "keywords": [
    "jali",
    "microservice"
  ],
  "author": "Latticework",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "bugs": {
    "url": "https://github.com/latticework/jali/issues"
  },
  "homepage": "http://jali-ms.io/",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "core-js": "^2.4.0",
    "espower": "^1.3.2",
    "systemjs": "^0.19.31",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "@types/node": "^4.0.28-alpha",
    "ava": "^0.15.2",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-plugin-transform-function-sent": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.9.0",
    "codelyzer": "^0.0.25",
    "mkdirp": "^0.5.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0-dev.0",
    "typescript": "^2.0.0-dev.20160705",
    "typings": "^1.3.1",
    "webpack": "^2.1.0-beta.15",
    "webpack-merge": "^0.14.0"
  },
  "babel": {
    "presets": [],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-async-to-generator",
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-es2015-function-name",
      "transform-function-sent",
      "transform-exponentiation-operator",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": false
        }
      ],
      "espower"
    ],
    "env": {
      "development": {},
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      },
      "production": {}
    }
  },
  "ava": {
    "//": "See https://github.com/avajs/ava/blob/master/docs/recipes/babelrc.md#transpiling-tests-and-sources-the-same-way.",
    "babel": {
      "plugins": [
        "syntax-trailing-function-commas",
        "transform-async-to-generator",
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-es2015-function-name",
        "transform-function-sent",
        "transform-exponentiation-operator",
        "transform-es2015-modules-commonjs",
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": false
          }
        ],
        "espower"
      ]
    },
    "require": [
      "babel-register"
    ]
  }
}
