{
  "//": [
    "babel:",
    "for ts+babel sourcemaps see http://stackoverflow.com/a/37799745/2240669",
    "ava:",
    "See https://github.com/avajs/ava/blob/master/docs/recipes/babelrc.md#transpiling-tests-and-sources-the-same-way.",
    "See https://github.com/avajs/ava/blob/master/docs/recipes/code-coverage.md"
  ],
  "name": "jali-srcs",
  "version": "0.0.0",
  "description": "Cross-platform service specification and execution context. Jali is the JavaScript implementation.",
  "scripts": {
    "build": "npm run build:dev",
      "build:packages": "tsc -p packages",
        "postbuild:packages": "npm run build:examples",
      "build:examples": "tsc -p examples",
      "build:dev": "npm run build:packages -- --sourceMap --inlineSources",
        "prebuild:dev": "npm run clean",
      "build:test": "npm run build:packages -- --inlineSourceMap --inlineSources",
        "prebuild:test": "npm run clean",
      "build:prod": "npm run build:packages -- --sourceMap --inlineSources",
        "prebuild:prod": "npm run clean",
    "clean": "npm run clean:dist && npm run clean:coverage",
      "clean:dist": "rimraf ./dist",
      "clean:coverage": "rimraf ./coverage",
      "clean:all": "npm cache clean && rimraf node_modules coverage",
        "preclean:all": "npm run clean",
    "cover": "BABEL_ENV=test nyc ava --tap --concurrency 5",
      "precover": "npm run build:test",
      "postcover": "nyc report",
      "cover:html": "xdg-open ./coverage/index.html",
        "precover:html": "npm run cover",
    "docs": "npm run docs:es",
      "predocs": "npm run build",
      "docs:dg": "echo ⚠ WARNING: Not operational script ⚠ && dgeni ./config/dgeni/index.js",
      "docs:es": "esdoc -c esdoc.json",
      "docs:td": "echo ⚠ WARNING: Not operational script ⚠ && typedoc --options typedoc.json",
      "docs:html": "xdg-open ./dist/docs/index.html",
        "predocs:html": "npm run docs",
    "e2e": "protractor",
      "pree2e": "webdriver-manager update",
    "examples": "NODE_PATH=$NODE_PATH:./dist/examples/packages node ./dist/examples/examples/index.js",
      "preexamples": "npm run build",
    "install:clean": "npm set progress=false && npm install",
      "preinstall:clean": "npm run clean",
    "lint": "tslint \"packages/**/*.ts\"",
    "start": "ng server",
      "start:clean": "npm start",
        "prestart:clean": "npm run clean",
    "test": "BABEL_ENV=test ava --tap --concurrency 5",
      "pretest": "npm run build:test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/latticework/jali.git"
  },
  "keywords": [
    "jali",
    "microservice"
  ],
  "author": "Latticework",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "bugs": {
    "url": "https://github.com/latticework/jali/issues"
  },
  "homepage": "http://jali-ms.io/",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "core-js": "^2.4.0",
    "disposables": "^1.0.1",
    "espower": "^1.3.2",
    "reflect-metadata": "^0.1.3",
    "systemjs": "^0.19.31",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "@types/node": "^4.0.28-alpha",
    "ava": "^0.15.2",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-plugin-transform-function-sent": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.9.0",
    "codelyzer": "^0.0.26",
    "dgeni": "^0.4.2",
    "dgeni-packages": "^0.14.0",
    "esdoc": "^0.4.7",
    "mkdirp": "^0.5.1",
    "nyc": "^7.0.0",
    "rmrf": "^1.0.2",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0-dev.0",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.0-dev.20160705",
    "webpack": "^2.1.0-beta.15",
    "webpack-merge": "^0.14.0"
  },
  "babel": {
    "env": {
      "development": {},
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ],
        "sourceMaps": "inline"
      },
      "production": {}
    },
    "presets": [],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-async-to-generator",
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-es2015-function-name",
      "transform-function-sent",
      "transform-exponentiation-operator",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": false
        }
      ],
      "espower"
    ],
    "ignore": [
      "*.test.js"
    ],
    "sourceMaps": "inline"
  },
  "ava": {
    "files": [
      "dist/all/**/*.test.js"
    ],
    "concurrency": 5,
    "babel": {
      "plugins": [
        "syntax-trailing-function-commas",
        "transform-async-to-generator",
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-es2015-function-name",
        "transform-function-sent",
        "transform-exponentiation-operator",
        "transform-es2015-modules-commonjs",
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": false
          }
        ],
        "espower"
      ]
    },
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.test.*",
      "**/testing/*"
    ],
    "reporter": [
      "html",
      "cobertura"
    ]
  }
}
